rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /tests/{test} {
      allow read, write: if true;
      
      match /exercises/{exercise} {
        allow read, write: if true;
      }
    }
    match /correctAnswers/{document} {
      allow read, write: if true;
    }
  }
}
